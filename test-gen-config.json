{
  "enabled": "true",
  "increase-coverage": {
    "project-name": "recipe-app-api",
    "target-branches": [
      "master"
    ],
    "test-env-docker-stage": "test",
    "build-test-env-docker-stage-script-path": "tool/build-test-env-docker-stage.sh",
    "test-files-folders": [
      "recipe-app-api/app/core/tests"
    ],
    "source-test-file-paths": [
      {
        "source-file-path": "recipe-app-api/app/core/models.py",
        "test-file-path": "recipe-app-api/app/core/tests/test_models.py"
      }
    ],
    "test-command": "pytest --cov=core --cov-report=xml  --cov-report=term",
    "coverage-type": "cobertura",
    "code-coverage-report-path": "recipe-app-api/app/core/tests/coverage.xml",
    "desired-coverage": 100,
    "max-iterations": 1,
    "max-number-of-test-cases": 6
    }
}